    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch DynawoAlgorithms",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build-code/install/dynawo-algorithms/bin/dynawoAlgorithms",
            "args": [ "--simulationType", "MC", "--input", "fic_MULTIPLE.xml", "--output", "output.zip", "--directory", "${workspaceFolder}/nrt/data/IEEE14/MC/files" ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                { "name": "DYNAWO_ALGORITHMS_LOCALE", "value" : "en_GB" },
                { "name": "DYNAWO_INSTALL_DIR", "value" : "${workspaceFolder}/../dynawo/build-code/deploy" },
                { "name": "DYNAWO_DDB_DIR", "value" : "${workspaceFolder}/../dynawo/build-code/deploy/ddb" },
                { "name": "DYNAWO_RESOURCES_DIR", "value" : "${workspaceFolder}/build-code/install/dynawo-algorithms/share:${workspaceFolder}/../dynawo/build-code/deploy/share:${workspaceFolder}/build-code/install/dynawo-algorithms/share/xsd:${workspaceFolder}/../dynawo/build-code/deploy/share/xsd" },
                { "name": "DYNAWO_DICTIONARIES", "value" : "dictionaries_mapping" },
                { "name": "DYNAWO_USE_XSD_VALIDATION", "value" : "false" },
                { "name": "DYNAWO_LIBIIDM_EXTENSIONS", "value" : "${workspaceFolder}/../dynawo/build-code/deploy/lib" },
                { "name": "IIDM_XML_XSD_PATH", "value" : "${workspaceFolder}/../dynawo/build-code/deploy/share/iidm/xsd/" },
                { "name": "LD_LIBRARY_PATH", "value" : "${env:LD_LIBRARY_PATH}:${workspaceFolder}/build-code/install/3rdParty/gperftools/lib:${workspaceFolder}/build-code/install/3rdParty/mpich/lib:${workspaceFolder}/build-code/install/dynawo-algorithms/lib:${workspaceFolder}/../dynawo/build-code/deploy/lib" }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Activer l'impression en mode Pretty pour gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Définir la version désassemblage sur Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "(gdb) Launch unittest: MultipleJobs",
            "type": "cppdbg",
